<Project>
  <PropertyGroup>
    <UseWPF>true</UseWPF>
    <PackageIcon>logo.png</PackageIcon>
    <!--<TargetFrameworks>net6.0-windows;net7.0-windows;</TargetFrameworks>
		<GeneratePackageOnBuild>True</GeneratePackageOnBuild>-->
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <TargetFrameworks>net8.0-windows</TargetFrameworks>
    <GeneratePackageOnBuild>False</GeneratePackageOnBuild>
    <LangVersion>10.0</LangVersion>
    <Version>2.0.0</Version>
    <FileVersion>2.0.0</FileVersion>
    <AssemblyVersion>2.0.0</AssemblyVersion>
    <Copyright>Copyright © HeBianGu 2025-2025</Copyright>
    <Authors>HeBianGu </Authors>
    <RepositoryUrl>https://github.com/HeBianGu/WPF-VisionMaster</RepositoryUrl>
    <PackageProjectUrl>https://github.com/HeBianGu/WPF-VisionMaster</PackageProjectUrl>
    <PackageReadmeFile>README.md</PackageReadmeFile>
    <Description>使用OpenCVSharp框架和参考VisionMaster操作界面的产品</Description>
    <!--<GenerateDocumentationFile>True</GenerateDocumentationFile>-->
  </PropertyGroup>
  
  <ItemGroup>
    <Resource Include="**\*.png" />
    <Resource Include="**\*.jpg" />
    <Resource Include="**\*.cur" />
    <Resource Include="**\*.ico" />
    <Resource Include="**\*.bmp" />
  </ItemGroup>
  
  <ItemGroup>
    <None Include="..\..\..\Document\logo.png">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
  <ItemGroup>
    <None Include="..\..\..\README.md">
      <Pack>True</Pack>
      <PackagePath>\</PackagePath>
    </None>
  </ItemGroup>
</Project>